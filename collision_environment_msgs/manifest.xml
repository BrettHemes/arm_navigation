  </description>
  <author>Sachin Chitta</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/collision_environment_msgs</url>
  <depend package="actionlib_msgs"/>
 <export>
