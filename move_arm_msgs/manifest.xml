<package>
  <description brief="move_arm_msgs">
    This package contains the messages needed to communicate with the move_arm action through an action interface.
  </description>
  <author>Sachin Chitta</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/move_arm_msgs</url>

  <depend package="actionlib_msgs"/>
  <depend package="motion_planning_msgs"/>
  <depend package="geometry_msgs"/>
  <depend package="planning_environment_msgs"/>
  <depend package="trajectory_msgs"/>
  
  <export>
    <cpp cflags="-I${prefix}/msg/cpp -I${prefix}/include"/>
  </export>

  <platform os="ubuntu" version="9.04"/>
  <platform os="ubuntu" version="9.10"/>
  <platform os="ubuntu" version="10.04"/>
</package>


