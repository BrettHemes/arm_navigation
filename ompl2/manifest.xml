<package>
  <description brief="Open Motion Planning Library (OMPL)">
    A library of sampling-based motion planning algorithms

  </description>
  <author>Ioan Sucan/isucan@willowgarage.com</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/ompl</url>
  
  <depend package="rosconsole"/>
  
  <export>
    <cpp cflags="-I${prefix}/build/ompl/code `rosboost-cfg --cflags`" lflags="-Wl,-rpath,${prefix}/lib -L${prefix}/lib -lompl `rosboost-cfg --lflags date_time` `rosboost-cfg --lflags thread`"/>
  </export>
  
</package>
